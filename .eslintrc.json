{
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb",
    "airbnb-typescript",
    "plugin:import/typescript",
    "plugin:import/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": [
      "./tsconfig.json"
    ]
  },
  "plugins": [
    "react",
    "@typescript-eslint",
    "import"
  ],
  "rules": {
    "eol-last": [
      "warn",
      "always"
    ],
    "arrow-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "comma-dangle": [
      "warn",
      "never"
    ],
    "jsx-quotes": [
      "warn",
      "prefer-single"
    ],
    "key-spacing": [
      "warn",
      {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict"
      }
    ],
    "keyword-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "no-console": [
      "warn",
      {
        "allow": [
          "error",
          "info"
        ]
      }
    ],
    "no-debugger": "warn",
    "prefer-template": "warn",
    "quotes": [
      "warn",
      "single"
    ],
    "react/jsx-closing-tag-location": "warn",
    "semi": "warn",
    "space-before-blocks": [
      "warn",
      "always"
    ],
    "space-infix-ops": [
      "warn",
      {
        "int32Hint": false
      }
    ],
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "template-curly-spacing": "off",
    "indent": [
      "warn",
      2,
      {
        "ignoredNodes": [
          "TemplateLiteral"
        ]
      }
    ],
    "import/no-anonymous-default-export": [
      "warn",
      {
        "allowAnonymousFunction": true,
        "allowObject": true
      }
    ],
    "prefer-const": "warn"
  }
}